# COMPOSER
COMPOSER_VERSION=1.8.6

# ELASTIC SEARCH
ES_VERSION=7.8.0

# CONTAINER NAME
PHP=php-jb
WEBSERVER=webserver-jb
MYSQL=mysql-jb
REDIS=redis-jb
ELASTIC_SEARCH=es-jb
